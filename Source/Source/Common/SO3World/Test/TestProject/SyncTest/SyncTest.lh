local GAME_FPS   = 16
local nSrcX      = 0.5 * 2048 -- 第一个region的正中心
local nSrcY      = 0.5 * 2048 -- 第一个region的正中心
local nPlayerID1 = 1
local nPlayerID2 = 2
local nTestIndex = 1
local tTestCase = {}
local SetPositionTimer = 1 -- SetPosition的延迟时间
local scene1 = nil



fSyncTest = function()
	scene1 = nil
	nTestIndex = 82
	local player1, player2 = TestBegin()
	scene1 = player2.GetScene()
	scene1.SetTimer(GAME_FPS * 20, "scripts/Include/SyncTest.lh", 0, 0)
end

function GetDirection(nX, nY, nX1, nY1)
    local nface = 0

    if nX > nX1 and nY > nY1 then
        nface = math.atan((nY1 - nY) / (nX1 - nX)) / math.pi * 128 + 128
    elseif nX > nX1 and nY < nY1 then
        nface = math.atan((nY1 - nY) / (nX1 - nX)) / math.pi * 128 + 128
    elseif nX < nX1 and nY > nY1 then
        nface = math.atan((nY1 - nY) / (nX1 - nX)) / math.pi * 128 + 256
    elseif nX < nX1 and nY < nY1 then
        nface = math.atan((nY1 - nY) / (nX1 - nX)) / math.pi * 128
    elseif nX == nX1 and nY < nY1 then
        nface = 64
    elseif nY == nY1 and nX < nX1 then
        nface = 0
    elseif nX == nX1 and nY > nY1 then
        nface = 192
    elseif nY == nY1 and nX > nX1 then
        nface = 128
    end

    return nface
end


function TestBegin()
    local player1 = GetPlayer(nPlayerID1)
    if not player1 then
        return nil, nil
    end

    local player2 = GetPlayer(nPlayerID2)
    if not player2 then
        return nil, nil
    end

    player1.SetPosition(nSrcX, nSrcY, 0)
    player2.SetPosition(nSrcX, nSrcY, 0)

    return player1, player2
end

function TestEnd(player1, player2)
    player1.Revive()
    player2.Revive()
    player1.SetPosition(nSrcX, nSrcY, 0)
    player2.SetPosition(nSrcX, nSrcY, 0)
end

function OnTimer(scene, nParam1, nParam2)
    local player1 = GetPlayer(nPlayerID1)
    if not player1 then
        return
    end

    local player2 = GetPlayer(nPlayerID2)
    if not player2 then
        return
    end


		if nParam2 == 0 then

	    local string1 = "[SYNCTEST]Player1:(" .. player1.nX .. "," .. player1.nY .. ")"
	    local string2 = "[SYNCTEST]Player2:(" .. player2.nX .. "," .. player2.nY .. ")"
	    local string3 = "[SYNCTEST]TestCase " .. nTestIndex - 1 .. " end.."

	    Log(string1)
	    Log(string2)
	    if nTestIndex > 1 then
	        Log(string3)
	    end

	    TestEnd(player1, player2)

	    TestBegin()

	    if nTestIndex == #tTestCase + 1 then
	    	return
	    end
	    if nTestIndex == 86 then
				nTestIndex = nTestIndex + 1
			end
	    tTestCase[nTestIndex](player1, player2, 0)
	  else
	  	tTestCase[nTestIndex](player1, player2, 1)
	  end
end

function RegionTest(player1, player2, nPlayer1DestX, nPlayer1DestY, bPlayer1Move, nPlayer2DestX, nPlayer2DestY, bPlayer2Move)
    local nVelocityXY  = 50
    local nDistance    = math.sqrt((player1.nX - nPlayer1DestX) ^ 2 + (player1.nY - nPlayer1DestY) ^ 2)
    local nDirectionXY = GetDirection(player1.nX, player1.nY, nPlayer1DestX, nPlayer1DestY)
    local nVelocityZ   = 0
    local nFrame       = nDistance / nVelocityXY

    if ((nDistance % nVelocityXY) ~= 0) then
        nFrame = nFrame + 1
    end

    local string = "[SYNCTEST]TestCase " .. nTestIndex .. " begin.."
    Log(string)


    if bPlayer1Move then
        player1.Dash(nFrame, nDirectionXY, nVelocityXY, nVelocityZ)
    end

    nVelocityXY  = 20
    nDistance    = math.sqrt((player2.nX - nPlayer2DestX) ^ 2 + (player2.nY - nPlayer2DestY) ^ 2)
    nDirectionXY = GetDirection(player2.nX, player2.nY, nPlayer2DestX, nPlayer2DestY)
    nVelocityZ   = 0
    nFrame       = nDistance / nVelocityXY

    if ((nDistance % nVelocityXY) ~= 0) then
        nFrame = nFrame + 1
    end

    if bPlayer2Move then
        player2.Dash(nFrame, nDirectionXY, nVelocityXY, nVelocityZ)
    end

    nTestIndex = nTestIndex + 1
    scene1.SetTimer(GAME_FPS * 12, "scripts/Include/SyncTest.lh", 0, 0)
end

